{
     "name": "shadcn-tanstack-router",
     "private": true,
     "version": "1.0.0",
     "type": "module",
     "scripts": {
          "dev": "vite",
          "build": "tsc && vite build",
          "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && npx tsc --noEmit && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
          "test": "vitest",
          "coverage": "vitest run --coverage",
          "preview": "vite preview",
          "format": "prettier --write ."
     },
     "lint-staged": {
          "*.{ts,tsx}": [
               "prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
               "eslint --max-warnings 0 --no-warn-ignored ."
          ]
     },
     "dependencies": {
          "@radix-ui/react-avatar": "^1.1.10",
          "@radix-ui/react-dropdown-menu": "^2.1.15",
          "@radix-ui/react-icons": "^1.3.2",
          "@radix-ui/react-slot": "^1.0.2",
          "@tanstack/react-router": "^1.129.8",
          "class-variance-authority": "^0.7.1",
          "clsx": "^2.1.1",
          "lucide-react": "^0.321.0",
          "react": "^18.3.1",
          "react-dom": "^18.3.1",
          "tailwind-merge": "^2.6.0",
          "tailwindcss-animate": "^1.0.7"
     },
     "devDependencies": {
          "@tanstack/react-router-devtools": "^1.129.8",
          "@tanstack/router-vite-plugin": "^1.129.8",
          "@testing-library/react": "^16.3.0",
          "@testing-library/user-event": "^14.6.1",
          "@types/eslint__js": "^8.42.3",
          "@types/node": "^20.19.9",
          "@types/react": "^18.3.23",
          "@types/react-dom": "^18.3.7",
          "@typescript-eslint/eslint-plugin": "^6.21.0",
          "@typescript-eslint/parser": "^6.21.0",
          "@vitejs/plugin-react": "^4.7.0",
          "@vitest/coverage-v8": "^3.2.4",
          "autoprefixer": "^10.4.21",
          "eslint": "^8.57.1",
          "eslint-plugin-react-hooks": "^4.6.2",
          "eslint-plugin-react-refresh": "^0.4.20",
          "husky": "^9.1.7",
          "jsdom": "^26.1.0",
          "lint-staged": "^16.1.2",
          "postcss": "^8.5.6",
          "prettier-plugin-organize-imports": "^4.2.0",
          "prettier-plugin-tailwindcss": "^0.6.14",
          "tailwindcss": "^3.4.17",
          "typescript": "^5.8.3",
          "typescript-eslint": "^8.38.0",
          "vite": "^7.0.6",
          "vitest": "^3.2.4"
     }
}
